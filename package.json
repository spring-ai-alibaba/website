{
  "name": "docs-website-template",
  "version": "1.0.0",
  "private": true,
  "description": "基于 Docusaurus 的文档网站模板",
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "scripts": {
    "docusaurus": "docusaurus",
    "dev": "docusaurus start",
    "start": "docusaurus start",
    "start:en": "docusaurus start --locale en",
    "start:zh": "docusaurus start --locale zh-Hans",
    "build": "docusaurus build",
    "build:prod": "docusaurus build && ./scripts/create-redirects.sh",
    "build:en": "docusaurus build --locale en",
    "build:zh": "docusaurus build --locale zh-Hans",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-translations:en": "docusaurus write-translations --locale en",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "documentation",
    "website",
    "template",
    "docusaurus",
    "dual-theme",
    "open-source"
  ],
  "dependencies": {
    "@docusaurus/core": "^3.8.1",
    "@docusaurus/plugin-client-redirects": "^3.8.1",
    "@docusaurus/preset-classic": "^3.8.1",
    "@docusaurus/theme-mermaid": "^3.8.1",
    "@mdx-js/react": "^3.1.0",
    "clsx": "^2.1.1",
    "fuse.js": "^7.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.9.1",
    "@docusaurus/tsconfig": "^3.9.1",
    "@docusaurus/types": "^3.9.1",
    "@stylistic/eslint-plugin": "2.13.0",
    "@types/react": "^19.1.14",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "9.18.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "typescript": "^5.9.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
